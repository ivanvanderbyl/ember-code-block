{
	"name": "ember-code-block",
	"version": "2.0.7",
	"description": "Simple, effective, and customizable code highlighting for Ember",
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"scripts": {
		"build": "ember build",
		"start": "ember server",
		"test": "ember test",
		"test:all": "ember try:each"
	},
	"repository": "ivanvanderbyl/ember-code-block",
	"engines": {
		"node": ">= 6.0.0"
	},
	"author": "Ivan Vanderbyl",
	"license": "MIT",
	"devDependencies": {
		"broccoli-asset-rev": "^2.4.5",
		"ember-ajax": "^3.0.0",
		"ember-cli": "~3.1.4",
		"ember-cli-changelog": "^0.3.4",
		"ember-cli-dependency-checker": "^2.0.0",
		"ember-cli-eslint": "^4.2.1",
		"ember-cli-htmlbars-inline-precompile": "^1.0.0",
		"ember-cli-inject-live-reload": "^1.4.1",
		"ember-cli-qunit": "^4.3.2",
		"ember-cli-release": "^1.0.0-beta.2",
		"ember-cli-shims": "^1.2.0",
		"ember-cli-sri": "^2.1.0",
		"ember-cli-uglify": "^2.0.0",
		"ember-cli-update": "^0.21.4",
		"ember-disable-prototype-extensions": "^1.1.2",
		"ember-export-application-global": "^2.0.0",
		"ember-load-initializers": "^1.0.0",
		"ember-maybe-import-regenerator": "^0.1.6",
		"ember-resolver": "^4.0.0",
		"ember-source": "^3.1",
		"ember-source-channel-url": "^1.0.1",
		"ember-try": "^0.2.23",
		"eslint-plugin-ember": "^5.0.0",
		"eslint-plugin-node": "^6.0.1",
		"loader.js": "^4.2.3"
	},
	"keywords": ["ember-addon", "code-highlighting", "code-block", "code-snippet"],
	"dependencies": {
		"broccoli-file-creator": "^1.1.1",
		"broccoli-funnel": "^1.0.3",
		"broccoli-merge-trees": "^1.1.2",
		"ember-cli-babel": "^6.6.0",
		"ember-cli-cjs-transform": "^1.2.0",
		"ember-cli-htmlbars": "^2.0.1",
		"highlight.js": "^9.8.0"
	},
	"ember-addon": {
		"configPath": "tests/dummy/config"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	}
}
